{
  "name": "ticketlab",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "prettier:check": "npx prettier --check 'src/**/*.{tsx,ts}' ",
    "prettier:fix": "npx pretty-quick --staged",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "lint:ext": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "6.3.2",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/stack": "6.3.3",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-fast-image": "8.6.1",
    "react-native-gesture-handler": "2.5.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-reanimated": "2.10.0",
    "react-native-safe-area-context": "4.3.3",
    "react-native-screens": "3.15.0",
    "react-native-shadow-2": "7.0.6",
    "react-native-skeleton-placeholder": "5.2.3",
    "react-native-svg": "13.1.0",
    "react-native-uuid": "2.0.1",
    "react-native-vector-icons": "9.2.0",
    "responsive-native": "1.0.2",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-arrow-functions": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@babel/plugin-transform-shorthand-properties": "7.18.6",
    "@babel/plugin-transform-template-literals": "7.18.9",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@storybook/addon-actions": "5.3",
    "@storybook/addon-knobs": "5.3",
    "@storybook/addon-links": "5.3",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-backgrounds": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.26",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-native": "5.1.2",
    "@testing-library/react-native": "11.3.0",
    "@tsconfig/react-native": "2.0.2",
    "@types/jest": "^29.2.1",
    "@types/mocha": "^10.0.0",
    "@types/react-native": "0.69.3",
    "@types/react-native-dotenv": "0.2.0",
    "@types/react-native-vector-icons": "6.4.11",
    "@types/react-test-renderer": "18.0.0",
    "@types/storybook__react": "5.2.1",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "5.1.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.5",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "git-commit-msg-linter": "4.2.1",
    "husky": "8.0.1",
    "jest": "26.6.3",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.73.2",
    "pretty-quick": "3.1.3",
    "react-dom": "18.1.0",
    "react-native-dotenv": "3.3.1",
    "react-test-renderer": "18.0.0",
    "typescript": "4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js",
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ]
  }
}
